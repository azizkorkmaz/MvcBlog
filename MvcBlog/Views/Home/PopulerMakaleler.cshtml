@model IEnumerable<MvcBlog.Models.Makale>

@{
    Layout = null;
    
}
<h3>En Çok Okunan Makaleler</h3>
<div class="row">
    <div class="col-lg-12">
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="/Home/MakaleDetay/@item.MakaleId" id="oku">@item.Baslik</a> (@item.Okunma)</li>
            }
        </ul>
    </div>
</div>
